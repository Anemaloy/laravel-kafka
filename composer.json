{
    "name": "anemaloy/laravel-kafka",
    "type": "library",
    "description": "Kafka transport for laravel",
    "license": "proprietary",
    "require": {
        "php": "^7.4 || ^8.2",
        "ext-json": "*",
        "opis/json-schema": "^1.0",
        "m1x0n/opis-json-schema-error-presenter": "^0.5.2",
        "ramsey/uuid": "^4.1",
        "composer/semver": "*"
    },
    "autoload": {
        "psr-4": {
            "Anemaloy\\LaravelKafka\\": "src/",
            "Anemaloy\\LaravelKafka\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/console": "*",
        "phpunit/phpunit": "^9.5 <= 9.5.16",
        "symplify/easy-coding-standard": "^10.0"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
